import java.util.*;

public class ArrayListProcessing
{
    public static void main(String[] args){
        ArrayList<String> sports = new ArrayList<String>();
        sports.add("Softball");
        sports.add("Soccer");
        sports.add("Basketball");
        sports.add("Tenis");
        sports.add("Skiing");
        sports.add("Rowing");
        
        System.out.println(returnElement(sports, 3));
        removeThirdElement(sports);
    }
    
    //1
    public static void printElements(ArrayList<String> inList){
        for(int i=0; i<inList.size(); i++)
            System.out.println(inList.get(i));
            
    }
    
    //2
    public static void addElement(ArrayList<String> inList){
        inList.add("Chess");
        for(int i=0; i<inList.size(); i++)
            System.out.println(inList.get(i));
            
    }
    
    //3
    public static String returnElement(ArrayList<String> inList, int i){
        return inList.get(i);
        
    }
    
    //4
    public static void removeThirdElement(ArrayList<String> inList){
        inList.remove(2);
        for(int i=0; i<inList.size(); i++)
            System.out.println(inList.get(i));
            
    }
    
    //5
    public static void findElement(ArrayList<String> inList, String word){
   
        for(int i=0; i<inList.size(); i++){
            if(word== inList(i)){
                System.out.println(inList.get(i));
            }
            }
        
    }
    
    
    
    
}
